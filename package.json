{
  "private": true,
  "scripts": {
    "build": "NODE_ICU_DATA=$(pwd)/node_modules/full-icu eleventy",
    "mp3": "cd mp3 && rsync -az --delete . web-standards.ru:/var/www/web-standards.ru/html/podcast/episodes/",
    "feed": "cd dist && rsync -az --delete index.xml web-standards.ru:/var/www/web-standards.ru/html/podcast/feed/",
    "deploy": "npm run mp3 && npm run feed"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "full-icu": "^1.3.0",
    "html-minifier": "^4.0.0",
    "markdown-it": "^9.1.0",
    "pretty-data": "^0.40.0"
  }
}
